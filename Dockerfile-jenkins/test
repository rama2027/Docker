pipeline
{
agent {
        docker { image 'amazonlinux' }
    }
stages
{
stage("python")
{
steps
{

sh 'python3 -m venv my_app/env'

sh 'source ~/my_app/env/bin/activate'

sh 'pip install pip --upgrade'

sh 'pip install boto3'

}
}
stage("Clone")
{
steps
{
git branch: 'test', url: 'https://github.com/rama2027/love.git'
}
}
stage("Credentials")
{
steps
{
sh 'python creds.py'
}
}
stage("create s3")
{
steps
{
sh 'sh $WORKSPACE/S3Creation.sh'
}
}
stage("upload s3")
{
steps
{
sh 'sh $WORKSPACE/upload.sh'
}
}
}
}
