pipeline
{
agent {
        docker { 
        image 'amazonlinux'
        args '-u root --privileged'
        }
    }
stages
{
stage("python")
{
steps
{
git branch: 'test', url: 'https://github.com/rama2027/love.git'
sh '$WORKSPACE@script/Dockerfile-jenkins/py.sh'
}
}
stage("Clone")
{
steps
{
git branch: 'test', url: 'https://github.com/rama2027/love.git'
}
}
stage("Credentials")
{
steps
{
sh 'python creds.py'
}
}
stage("create s3")
{
steps
{
sh 'sh $WORKSPACE/S3Creation.sh'
}
}
stage("upload s3")
{
steps
{
sh 'sh $WORKSPACE/upload.sh'
}
}
}
}
